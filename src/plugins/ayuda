sub {
    my ($self, $msg) = @_;

    my $help = "/clima - Clima de República Dominicana\n";
    $help .= "/top <año|all> - Top de mmsgs\n";
    $help .= "/temblo - Temblores de tierra recientes en República Dominicana\n";
    $help .= "/got - Cuenta regresiva proximo episodio de GoT\n";

    $self->sendMessage([
        chat_id => $msg->{chat}{id},
        text => $help
    ]);
}
